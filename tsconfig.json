{
  "compilerOptions": {
    "jsx": "react-jsx", // ✅ ou "react" selon ta version
    "lib": ["dom", "esnext"], // ✅ pour que TS connaisse les éléments du DOM
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
